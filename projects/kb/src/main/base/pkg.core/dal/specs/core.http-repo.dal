        table pkg_http_request_methods{
            row GET
                key = "GET"
                http:method = "GET"
                default:description_locale = "%Requests a representation of the specified resource. Note that GET should not be used for operations that cause side-effects, such as using it for taking actions in web applications. One reason for this is that GET may be used arbitrarily by robots or crawlers, which should not need to consider the side effects that a request should cause. See safe methods below."
                ;
            row POST
                key = "POST"
                http:method = "POST"
                default:description_locale = "%Submits data to be processed (e.g., from an HTML form) to the identified resource. The data is included in the body of the request. This may result in the creation of a new resource or the updates of existing resources or both."
                ;
            row OPTIONS
                key = "OPTIONS"
                http:method = "OPTIONS"
                default:description_locale = ""
                ;
            row HEAD
                key = "HEAD"
                http:method = "HEAD"
                default:description_locale = "%Asks for the response identical to the one that would correspond to a GET request, but without the response body. This is useful for retrieving meta-information written in response headers, without having to transport the entire content."
                ;
            row PUT
                key = "PUT"
                http:method = "PUT"
                default:description_locale = "%Uploads a representation of the specified resource."
                ;
            row DELETE
                key = "DELETE"
                http:method = "DELETE"
                default:description_locale = "%Deletes the specified resource."
                ;
            row TRACE
                key = "TRACE"
                http:method = "TRACE"
                default:description_locale = "%Echoes back the received request, so that a client can see what intermediate servers are adding or changing in the request."
                ;
            row CONNECT
                key = "CONNECT"
                http:method = "CONNECT"
                default:description_locale = "%Converts the request connection to a transparent TCP/IP tunnel, usually to facilitate SSL-encrypted communication (HTTPS) through an unencrypted HTTP proxy."
                ;
        }

        table pkg_http_status_codes{
            row sc100
                key = "100"
                http:status:code = "100"
                http:status:code:text = "Continue"
                http:category = "1xx Informational"
                http:protocol = "HTTP/1.0,HTTP/1.1"
                default:description_locale = "%This means that the server has received the request headers, and that the client should proceed to send the request body (in the case of a request for which a body needs to be sent; for example, a POST request). If the request body is large, sending it to a server when a request has already been rejected based upon inappropriate headers is inefficient. To have a server check if the request could be accepted based on the requests headers alone, a client must send Expect: 100-continue as a header in its initial request and check if a 100 Continue status code is received in response before continuing (or receive 417 Expectation Failed and not continue)."
                ;
            row sc101
                key = "101"
                http:status:code = "101"
                http:status:code:text = "Switching Protocols"
                http:category = "1xx Informational"
                http:protocol = "HTTP/1.0,HTTP/1.1"
                default:description_locale = "%This means the requester has asked the server to switch protocols and the server is acknowledging that it will do so."
                ;
            row sc102
                key = "102"
                http:status:code = "102"
                http:status:code:text = "Processing"
                http:category = "1xx Informational"
                http:protocol = "HTTP/1.0,HTTP/1.1"
                default:description_locale = "%As a WebDAV request may contain many sub-requests involving file operations, it may take a long time to complete the request. This code indicates that the server has received and is processing the request, but no response is available yet. This prevents the client from timing out and assuming the request was lost."
                ;
            row sc200
                key = "200"
                http:status:code = "200"
                http:status:code:text = "OK"
                http:category = "2xx Success"
                http:protocol = "HTTP/1.0,HTTP/1.1"
                default:description_locale = "%Standard response for successful HTTP requests. The actual response will depend on the request method used. In a GET request, the response will contain an entity corresponding to the requested resource. In a POST request the response will contain an entity describing or containing the result of the action."
                ;
            row sc201
                key = "201"
                http:status:code = "201"
                http:status:code:text = "Created"
                http:category = "2xx Success"
                http:protocol = "HTTP/1.0,HTTP/1.1"
                default:description_locale = "%The request has been fulfilled and resulted in a new resource being created."
                ;
            row sc202
                key = "202"
                http:status:code = "202"
                http:status:code:text = "Accepted"
                http:category = "2xx Success"
                http:protocol = "HTTP/1.0,HTTP/1.1"
                default:description_locale = "%The request has been accepted for processing, but the processing has not been completed. The request might or might not eventually be acted upon, as it might be disallowed when processing actually takes place."
                ;
            row sc203
                key = "203"
                http:status:code = "203"
                http:status:code:text = "Non-Authoritative Information"
                http:category = "2xx Success"
                http:protocol = "HTTP/1.1"
                default:description_locale = "%The server successfully processed the request, but is returning information that may be from another source."
                ;
            row sc204
                key = "204"
                http:status:code = "204"
                http:status:code:text = "No Content"
                http:category = "2xx Success"
                http:protocol = "HTTP/1.0,HTTP/1.1"
                default:description_locale = "%The server successfully processed the request, but is not returning any content."
                ;
            row sc205
                key = "205"
                http:status:code = "205"
                http:status:code:text = "Reset Content"
                http:category = "2xx Success"
                http:protocol = "HTTP/1.0,HTTP/1.1"
