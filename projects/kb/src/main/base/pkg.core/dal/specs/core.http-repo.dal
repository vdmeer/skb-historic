                key = "If-None-Match"
                http:header = "If-None-Match"
                default:example = "If-None-Match: '737060cd8c284d8af7ad3082f209582d'"
                default:description_locale = "%Allows a 304 Not Modified to be returned if content is unchanged, see HTTP ETag"
                ;
            row If-Range
                key = "If-Range"
                http:header = "If-Range"
                default:example = "If-Range: '737060cd8c284d8af7ad3082f209582d'"
